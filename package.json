{
  "name": "bies-support",
  "displayName": "bies-support",
  "description": "bies language support",
  "author": "Joshua Yarit Amador Lara, Fabiola Rojas Alvarado, Mauricio Chaves Chaves [Coordinador], Eddy Villarreal Mu√±oz",
  "license": "sin licencia",
  "repository": {
    "type": "git",
    "url": "https://github.com/MauricioCh2/bies-support.git"
  },
  "version": "0.0.1",
  "icon": "./images/logo.png",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "languages": [{
      "id": "bies",
      "extensions": [".bies"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "bies",
      "scopeName": "source.bies",
      "path": "./syntaxes/bies.tmLanguage.json"
    }],
    "snippets": [{
      "language": "bies",
      "path": "./snippets/snippets.code-snippets"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
